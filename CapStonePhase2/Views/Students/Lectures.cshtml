@model IEnumerable<CapStonePhase2.Models.Lectures>

@{
    ViewBag.Title = "Lectures";
}

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Topic)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Topic, "Description", "Lectures", new { studentid = item.StudentId, lectureid = item.Id }, null)
        </td>
        <td>
            @Html.ActionLink("View Grade", "Grade", new { studentid = item.StudentId, lectureid = item.Id })
        </td>
    </tr>
}

</table>
